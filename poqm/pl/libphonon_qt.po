#: audiooutput.cpp:481 audiooutput.cpp:519
#, qt-format
msgctxt "Phonon::AudioOutput|"
msgid ""
"<html>The audio playback device <b>%1</b> does not work.<br/>Falling back to "
"<b>%2</b>.</html>"
msgstr ""
"<html>Urządzenie do odtwarzania dźwięku <b>%1</b> nie działa.<br/"
">Przełączono na <b>%2</b>.</html>"

#: audiooutput.cpp:494
#, qt-format
msgctxt "Phonon::AudioOutput|"
msgid ""
"<html>Switching to the audio playback device <b>%1</b><br/>which just became "
"available and has higher preference.</html>"
msgstr ""
"<html>Przełączono na urządzenie do odtwarzania dźwięku <b>%1</b><br/>które "
"właśnie stało się dostępne i jest wyżej na liście wybieranych urządzeń.</"
"html>"

#: audiooutput.cpp:497 audiooutput.cpp:515
#, qt-format
msgctxt "Phonon::AudioOutput|"
msgid "Revert back to device '%1'"
msgstr "Wróć do urządzenia '%1'"

#: audiooutput.cpp:512
#, qt-format
msgctxt "Phonon::AudioOutput|"
msgid ""
"<html>Switching to the audio playback device <b>%1</b><br/>which has higher "
"preference or is specifically configured for this stream.</html>"
msgstr ""
"<html>Przełączono na urządzenie do odtwarzania dźwięku <b>%1</b><br/>które "
"jest wyżej na liście wybieranych urządzeń lub jest jednoznacznie ustawione "
"dla tego strumienia.</html>"

