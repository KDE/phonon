#: audiooutput.cpp:481 audiooutput.cpp:519
#, qt-format
msgctxt "Phonon::AudioOutput|"
msgid ""
"<html>The audio playback device <b>%1</b> does not work.<br/>Falling back to "
"<b>%2</b>.</html>"
msgstr ""
"<html>Perangkat pemutaran audio <b>%1</b> tidak bekerja.<br/>Kembali ke <b>"
"%2</b>.</html>"

#: audiooutput.cpp:494
#, qt-format
msgctxt "Phonon::AudioOutput|"
msgid ""
"<html>Switching to the audio playback device <b>%1</b><br/>which just became "
"available and has higher preference.</html>"
msgstr ""
"<html>Beralih ke peranti pemutaran audio <b>%1</b><br/>yang sekarang sudah "
"tersedia dan yang lebih disukai.</html>"

#: audiooutput.cpp:497 audiooutput.cpp:515
#, qt-format
msgctxt "Phonon::AudioOutput|"
msgid "Revert back to device '%1'"
msgstr "Balik kembali ke peranti '%1'"

#: audiooutput.cpp:512
#, qt-format
msgctxt "Phonon::AudioOutput|"
msgid ""
"<html>Switching to the audio playback device <b>%1</b><br/>which has higher "
"preference or is specifically configured for this stream.</html>"
msgstr ""
"<html>Beralih ke peranti pemutaran audio <b>%1</b><br/>yang paling disukai "
"atau yang dikonfigurasi secara khusus untuk stream ini.</html>"

