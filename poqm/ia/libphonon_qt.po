#: audiooutput.cpp:481 audiooutput.cpp:519
#, qt-format
msgctxt "Phonon::AudioOutput|"
msgid ""
"<html>The audio playback device <b>%1</b> does not work.<br/>Falling back to "
"<b>%2</b>.</html>"
msgstr ""
"<html>Le dispositivo de reproduction audio <b>%1</b> non functiona.<br/>Il "
"retorna a <b>%2</b>.</html>"

#: audiooutput.cpp:494
#, qt-format
msgctxt "Phonon::AudioOutput|"
msgid ""
"<html>Switching to the audio playback device <b>%1</b><br/>which just became "
"available and has higher preference.</html>"
msgstr ""
"<html>Commutar al dispositivo de reproduction audio <b>%1</b><br/> que justo "
"deveni disponibile e ha un preferentia major.</html>"

#: audiooutput.cpp:497 audiooutput.cpp:515
#, qt-format
msgctxt "Phonon::AudioOutput|"
msgid "Revert back to device '%1'"
msgstr "Reverte retro a dispositivo '%1'"

#: audiooutput.cpp:512
#, qt-format
msgctxt "Phonon::AudioOutput|"
msgid ""
"<html>Switching to the audio playback device <b>%1</b><br/>which has higher "
"preference or is specifically configured for this stream.</html>"
msgstr ""
"<html>Commutar al dispositivo de reproduction audio <b>%1</b><br/> que  ha "
"un preferentia major o es specificatemente configurate per iste fluxo.</html>"

